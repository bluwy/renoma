{
  "name": "renoma",
  "version": "0.1.7",
  "description": "Opinionated package health checks",
  "type": "module",
  "author": "Bjorn Lu",
  "license": "MIT",
  "bin": "./src/index.js",
  "scripts": {
    "cli": "node src/index.js",
    "lint": "prettier \"**/*.{js,md}\" --check",
    "format": "prettier \"**/*.{js,md}\" --write",
    "typecheck": "tsc",
    "test": "node --test test/index.js"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluwy/renoma.git"
  },
  "bugs": {
    "url": "https://github.com/bluwy/renoma/issues"
  },
  "packageManager": "pnpm@9.10.0",
  "dependencies": {
    "eslint": "^9.10.0",
    "eslint-plugin-depend": "^0.11.0",
    "eslint-plugin-regexp": "^2.6.0",
    "jsonc-eslint-parser": "^2.4.0",
    "picocolors": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  }
}
